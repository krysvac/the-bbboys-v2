{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "eslint:recommended",
        "google"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module",
        "project": "./tsconfig.json"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-unused-vars": "off",
        "new-cap": "off",
        "require-jsdoc": "off",
        "max-len": "off",
        "no-multiple-empty-lines": [
            "error",
            {
                "max": 1
            }
        ],
        "no-explicit-any": "off",
        "camelcase": "off"
    },
    "overrides": [
        {
            "files": [
                "**/**.spec.ts"
            ],
            "rules": {
                "no-undef": "off"
            }
        }
    ]
}
